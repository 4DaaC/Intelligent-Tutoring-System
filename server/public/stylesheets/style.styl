html,body
  background-color:#EEE
  height:100%
#bd
  padding: 20px 10px
  background-color:#FFF
  border:1px solid #555
  border-top:0
  -webkit-box-shadow: 0 1px 50px rgba(0, 0, 0, 0.5);
  -moz-box-shadow: 0 1px 50px rgba(0, 0, 0, 0.5);
  box-shadow: 0 1px 50px rgba(0, 0, 0, 0.5);
  position:relative;
  z-index:2;
#navbar
  -webkit-box-shadow: 0 1px 50px rgba(0, 0, 0, 0.5);
  -moz-box-shadow: 0 1px 50px rgba(0, 0, 0, 0.5);
  box-shadow: 0 1px 50px rgba(0, 0, 0, 0.5);
  position:relative;
  z-index:1;
  margin-bottom:0;
  width:960px;
  border-left:1px solid #555
  border-right:1px solid #555
  .navbar-inner
    border-radius:0;
    -moz-border-radius:0;
    -webkit-border-radius:0;
a
  color: #00B7FF
#error-box
  background-color:#FCC
  padding:20px
tr.correct, td.correct
  background:#DFD
tr.incorrect, td.incorrect
  background:#E77
#grades-table
  font-size:12px
td.pass
  color:green
td.fail
  color:red
th
  font-style:italic
.right
  float:right
